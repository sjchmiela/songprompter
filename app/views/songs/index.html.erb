<% content_for :title do %>Piosenki<% end %>

<a href="<%= new_song_path %>" class="card song-desc new-event">
  <div class="song">
    <h2>Nowa piosenka</h2>
  </div>
</a>
<% songs.each do |song| %>
<a href="<%= song_path(song) %>" class="card song-desc">
  <div class="song">
    <h2><%= song.name %></h2>
  </div>
</a>
<% end %>